CREATE TABLE student_activity (
    id INTEGER PRIMARY KEY,
    event_time DATETIME,
    course_name VARCHAR(255),
    activity_type VARCHAR(50),
    content_type VARCHAR(50),
    device_type VARCHAR(50),
    url VARCHAR(255),
    session_id INTEGER,
    path VARCHAR(255),
    student_id INTEGER
);

